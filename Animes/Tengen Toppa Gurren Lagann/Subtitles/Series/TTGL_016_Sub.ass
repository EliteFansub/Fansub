Dialogue: 0,0:02:25.77,0:02:28.31,Sign - Handwriting,,0,0,0,,VILA SUBTERRÃNEA.
Dialogue: 0,0:02:32.27,0:02:34.77,Sign - Handwriting,,0,0,0,,DIA APÓS DIA...
Dialogue: 0,0:02:37.11,0:02:39.61,Sign - Handwriting,,0,0,0,,ELE CAVA NA TERRA.
Dialogue: 0,0:02:42.95,0:02:45.45,Sign - Handwriting,,0,0,0,,SEUS PAIS MORRERAM.
Dialogue: 0,0:02:52.46,0:02:54.96,Sign - Handwriting,,0,0,0,,EM ALGUM LUGAR QUE NÃO ESTÁ AQUI...
Dialogue: 0,0:03:02.51,0:03:05.01,Sign - Handwriting,,0,0,0,,HÁ UM MUNDO ABERTO.
Dialogue: 0,0:03:12.19,0:03:14.65,Sign - Handwriting,,0,0,0,,ELE É FORTE, GENTIL E GRANDE.
Dialogue: 0,0:03:20.36,0:03:22.86,Sign - Handwriting,,0,0,0,,EU QUERO SER COMO O MEU ANIKI.
Dialogue: 0,0:03:34.75,0:03:37.25,Sign - Handwriting,,0,0,0,,ELE AINDA ERA UMA CRIANÇA.
Dialogue: 0,0:03:47.43,0:03:49.93,Sign - Handwriting,,0,0,0,,SEU PAI ESTÁ DE VOLTA.
Dialogue: 0,0:03:56.11,0:03:58.61,Sign - Handwriting,,0,0,0,,ELE ASSUME...
Dialogue: 0,0:04:11.50,0:04:14.00,Sign - Handwriting,,0,0,0,,UM GUNMEN INIMIGO.
Dialogue: 0,0:05:13.10,0:05:15.56,Sign - Handwriting,,0,0,0,,UMA MULHER PERIGOSA.
Dialogue: 0,0:05:19.61,0:05:22.11,Sign - Handwriting,,0,0,0,,UMA NOVA COMPANHEIRA.
Dialogue: 0,0:05:42.13,0:05:44.76,Sign - Handwriting,,0,0,0,,UM RIVAL PODEROSO...
Dialogue: 0,0:05:51.30,0:05:53.81,Sign - Handwriting,,0,0,0,,APARECE!
Dialogue: 0,0:06:27.67,0:06:30.18,Sign - Handwriting,,0,0,0,,UM CONFRONTO AO MEIO-DIA.
Dialogue: 0,0:06:58.62,0:07:01.12,Sign - Handwriting,,0,0,0,,ACHA QUE É GENTE SÓ PORQUE TEM DUAS CARAS!
Dialogue: 0,0:08:21.95,0:08:24.50,Sign - Handwriting,,0,0,0,,CAPITAL REAL
Dialogue: 0,0:08:30.71,0:08:33.22,Sign - Handwriting,,0,0,0,,REI ESPIRAL
Dialogue: 0,0:09:30.48,0:09:32.98,Sign - Handwriting,,0,0,0,,ASSUMA O CONTROLE DO DAI-GUNZAN!
Dialogue: 0,0:09:35.40,0:09:37.90,Sign - Handwriting,,0,0,0,,VOCÊ VAI FAZER ISSO!
Dialogue: 0,0:10:10.56,0:10:13.11,Sign - Handwriting,,0,0,0,,EU NÃO CONSIGO FAZER ISSO.
Dialogue: 0,0:11:02.70,0:11:05.16,Sign - Handwriting,,0,0,0,,EU NUNCA PODEREI SER COMO VOCÊ, ANIKI...
Dialogue: 0,0:12:37.29,0:12:39.79,Sign - Handwriting,,0,0,0,,A FILHA DO REI ESPIRAL...
Dialogue: 0,0:12:43.76,0:12:46.26,Sign - Handwriting,,0,0,0,,NIA.
Dialogue: 0,0:12:55.98,0:12:58.44,Sign - Handwriting,,0,0,0,,UM ENCONTRO EM MEIO AO DESESPERO.
Dialogue: 0,0:13:19.71,0:13:22.25,Sign - Handwriting,,0,0,0,,PERSEGUIÇÃO DO INIMIGO.
Dialogue: 0,0:13:46.45,0:13:48.95,Sign - Handwriting,,0,0,0,,PEDIDOS DE RECISÃO.
Dialogue: 0,0:14:21.56,0:14:24.07,Sign - Handwriting,,0,0,0,,SEU PAI DISSE QUE SE CANSOU DE VOCÊ.
Dialogue: 0,0:14:36.12,0:14:38.62,Sign - Handwriting,,0,0,0,,O VALE DAS PRINCESAS DESCARTADAS.
Dialogue: 0,0:15:11.86,0:15:14.41,Sign - Handwriting,,0,0,0,,SIMON VAI ME PROTEGER.
Dialogue: 0,0:15:38.64,0:15:41.14,Sign - Handwriting,,0,0,0,,ELE NÃO VAI PERDER PARA GENTE COMO VOCÊ.
Dialogue: 0,0:15:58.70,0:16:01.20,Sign - Handwriting,,0,0,0,,DESCULPE A DEMORA.
Dialogue: 0,0:16:13.47,0:16:15.97,Sign - Handwriting,,0,0,0,,ACREDITE EM VOCÊ!
Dialogue: 0,0:17:18.99,0:17:21.49,Sign - Handwriting,,0,0,0,,A BROCA INVENCÍVEL.
Dialogue: 0,0:17:37.72,0:17:40.22,Sign - Handwriting,,0,0,0,,O PODER DA ESPIRAL.
Dialogue: 0,0:17:52.61,0:17:55.11,Sign - Handwriting,,0,0,0,,OS HUMANOS DESPERTADOS...
Dialogue: 0,0:18:02.49,0:18:04.99,Sign - Handwriting,,0,0,0,,VÃO PARA A OFENSIVA.
Dialogue: 0,0:18:28.60,0:18:31.10,Sign - Handwriting,,0,0,0,,UMA DERROTA.
Dialogue: 0,0:19:55.06,0:19:57.57,Sign - Handwriting,,0,0,0,,EM DIREÇÃO À BATALHA FINAL...
Dialogue: 0,0:20:30.89,0:20:33.39,Sign - Handwriting,,0,0,0,,O REI ESPIRAL, LORDGENOME
Dialogue: 0,0:21:54.89,0:21:57.39,Sign - Handwriting,,0,0,0,,TEPPELIN, A CAPITAL REAL...
Dialogue: 0,0:22:03.32,0:22:05.82,Sign - Handwriting,,0,0,0,,CAIU.
Dialogue: 0,0:22:14.41,0:22:16.91,Sign - Handwriting,,0,0,0,,A HUMANIDADE...
Dialogue: 0,0:22:25.92,0:22:28.42,Sign - Handwriting,,0,0,0,,QUEBROU OS CÉUS.
Dialogue: 0,0:22:31.60,0:22:34.10,Sign - Handwriting,,0,0,0,,E ALÉM DISSO ESTÁ...
